{
  "name": "miniapp-game-heaven",
  "scripts": {
    "dev": "concurrently \"yarn:dev:*\"",
    "dev:web": "vite --host",
    "dev:emulators": "firebase emulators:start",
    "build": "tsc && vite build && cp ./dist/index.html ./dist/404.html",
    "zip": "yarn build && cd dist && zip -r Archive.zip ./",
    "preview": "vite preview",
    "preview:watch": "concurrently \"yarn:watch:*\"",
    "watch:tsc": "tsc --watch",
    "watch:vite": "vite build --watch",
    "watch:preview": "vite preview",
    "lint": "eslint ."
  },
  "dependencies": {
    "@heroicons/react": "^2.0.8",
    "@karrotmini/sdk": "^0.17.1",
    "events": "^3.3.0",
    "firebase": "^9.9.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.6.0"
  },
  "devDependencies": {
    "@types/events": "^3.0.0",
    "@types/node": "^18.7.13",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-helmet": "^6.1.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-react": "^2.0.1",
    "concurrently": "^7.3.0",
    "eslint": "^8.22.0",
    "eslint-config-kanziw": "^0.1.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "firebase-tools": "^11.8.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.9"
  },
  "packageManager": "yarn@3.2.2"
}
