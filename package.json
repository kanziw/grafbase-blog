{
  "name": "miniapp-game-heaven",
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "dev": "concurrently --kill-others \"yarn:dev:*\"",
    "dev:vite": "vite --host",
    "dev:grafbase": "npx grafbase dev",
    "codegen": "concurrently --kill-others \"yarn:codegen:*\"",
    "codegen:graphql": "graphql-codegen --config ./src/graphql/codegen.yml",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint ."
  },
  "dependencies": {
    "@urql/exchange-graphcache": "^5.0.0",
    "graphql": "^16.6.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "urql": "^3.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.11.6",
    "@graphql-codegen/typed-document-node": "^2.3.3",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.7.8",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@urql/core": "^3.0.0",
    "@vitejs/plugin-react": "^2.0.1",
    "concurrently": "^7.3.0",
    "eslint": "^8.22.0",
    "eslint-config-kanziw": "^0.1.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.9"
  }
}
