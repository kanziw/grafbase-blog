{
  "name": "miniapp-game-heaven",
  "version": "0.1.0",
  "scripts": {
    "dev": "concurrently \"yarn:dev:*\"",
    "dev:web": "vite --host",
    "dev:emulators": "firebase emulators:start",
    "build": "tsc && vite build",
    "zip": "yarn build && cd dist && zip -r Archive.zip ./",
    "preview": "vite preview",
    "preview:watch": "concurrently \"yarn:watch:*\"",
    "watch:tsc": "tsc --watch",
    "watch:vite": "vite build --watch",
    "watch:preview": "vite preview",
    "lint": "eslint ."
  },
  "dependencies": {
    "@heroicons/react": "^2.0.13",
    "@karrotmini/sdk": "^0.17.1",
    "events": "^3.3.0",
    "firebase": "^9.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.6.0"
  },
  "devDependencies": {
    "@types/events": "^3.0.0",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vitejs/plugin-react": "^3.0.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.30.0",
    "eslint-config-kanziw": "^0.1.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "firebase-tools": "^11.19.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.3"
  }
}
